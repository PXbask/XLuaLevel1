function OnInit()
    print("OnInit")
end
function OnShow()
    print("OnShow")
    self.transform.localPosition = Vector3(0,2,0)
end
function Update()
    print("Update")
    if Input.GetKey(KeyCode.W) then
        self.transform:Translate(self.transform.forward*Time.deltaTime*5)
    end
    if Input.GetKey(KeyCode.S) then
        self.transform:Translate(-self.transform.forward*Time.deltaTime*5)
    end
end
function OnHide()
    print("OnHide")
end